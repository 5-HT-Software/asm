{{#if flashMessages.error}}
  <script>
    toastr.error('{{flashMessages.error}}', 'Hata!');
  </script>
{{/if}}
{{#if flashMessages.success}}
  <script>
    toastr.success('{{flashMessages.success}}', 'Başarılı');
  </script>
{{/if}}
{{#if flashMessages.warning}}
  <script>
    toastr.warning('{{flashMessages.warning}}', 'Dikkat!');
  </script>
{{/if}}